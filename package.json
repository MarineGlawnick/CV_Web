{
    "name": "nuxt-app",
    "private": true,
    "scripts": {
        "build": "nuxt build",
        "dev": "nuxt dev",
        "generate": "nuxt generate",
        "preview": "nuxt preview",
        "postinstall": "nuxt prepare",
        "lint:eslint": "eslint --ext \".js,.ts,.vue\" --ignore-path .eslintignore --ignore-path .gitignore .",
