    "prettier": "^2.8.4",
    "@types/bootstrap": "^5.2.6",
    "nuxt": "^3.4.3",
    "sass": "^1.62.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "bootstrap": "^5.3.0"
  }
