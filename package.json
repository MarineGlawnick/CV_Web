    },
    "devDependencies": {
        "@nuxtjs/eslint-config-typescript": "^12.0.0",
        "@nuxtjs/i18n": "^8.0.0-rc.4",
        "@types/bootstrap": "^5.2.6",
        "@typescript-eslint/parser": "^5.53.0",
        "eslint": "^8.34.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "nuxt": "^3.4.3",
        "prettier": "^2.8.4",
        "sass": "^1.62.1",
        "typescript": "^5.0.4"
    },
    "dependencies": {
        "bootstrap": "^5.3.0",
        "bootstrap-icons": "^1.10.5"
    }
